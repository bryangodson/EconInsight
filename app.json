{
  "expo": {
    "name": "EconInsight",
    "slug": "EconInsight",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffafa"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.danquodes.econinsight"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Quicksand-Bold.otf",
            "./assets/fonts/Quicksand-light.otf",
            "./assets/fonts/Quicksand-Medium.otf",
            "./assets/fonts/Quicksand-Regular.otf",
            "./assets/fonts/Quicksand-SemiBold.otf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "9c0f5754-ae06-42f0-939a-9b97438a3c7b"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-5953493288912761~8143190614"
  }
}
